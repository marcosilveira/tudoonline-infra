client
proto udp
explicit-exit-notify
remote 217.216.81.157 1194
dev tun
resolv-retry infinite
nobind
persist-key
persist-tun
remote-cert-tls server
verify-x509-name server_hbgsmA7fnTfOEWNj name
auth SHA256
auth-nocache
cipher AES-128-GCM
ignore-unknown-option data-ciphers
data-ciphers AES-128-GCM
ncp-ciphers AES-128-GCM
tls-client
tls-version-min 1.2
tls-cipher TLS-ECDHE-ECDSA-WITH-AES-128-GCM-SHA256
tls-ciphersuites TLS_AES_256_GCM_SHA384:TLS_AES_128_GCM_SHA256:TLS_CHACHA20_POLY1305_SHA256
ignore-unknown-option block-outside-dns
setenv opt block-outside-dns # Prevent Windows 10 DNS leak
verb 3

# =============================================
# SPLIT TUNNEL - Internet direta, só redes
# internas do cluster passam pela VPN
# =============================================

# Ignora o redirect-gateway do servidor (não redireciona internet pela VPN)
route-nopull

# Rota para acessar o banco via VPN (Node 1 - Master/DB)
route 10.8.0.0 255.255.255.0

# Redes internas do cluster K3s (pods e services)
route 10.42.0.0 255.255.0.0
route 10.43.0.0 255.255.0.0

